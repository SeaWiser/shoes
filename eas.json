{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON_BASE64": "@GOOGLE_SERVICES_JSON_BASE64"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON_BASE64": "@GOOGLE_SERVICES_JSON_BASE64"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON_BASE64": "@GOOGLE_SERVICES_JSON_BASE64"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
